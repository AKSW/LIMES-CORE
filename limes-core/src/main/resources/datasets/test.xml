<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/1999/02/22-rdf-syntax-ns#</NAMESPACE>
		<LABEL>rdf</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2000/01/rdf-schema#</NAMESPACE>
		<LABEL>rdfs</LABEL>
	</PREFIX>
	<PREFIX>
		<NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
		<LABEL>owl</LABEL>
	</PREFIX>
	
<SOURCE>
		<ID>test</ID>
		<ENDPOINT>/home/abdullah/LIMES/limes-core/src/main/resources/datasets/test.nt</ENDPOINT>
		<VAR>?x</VAR>
		<PAGESIZE>1000</PAGESIZE>
		<RESTRICTION> </RESTRICTION>
		<PROPERTY>rdfs:label</PROPERTY>
		
		<TYPE>N3</TYPE>
</SOURCE>
	
	<TARGET>
		<ID>test</ID>
		<ENDPOINT>/home/abdullah/LIMES/limes-core/src/main/resources/datasets/test.nt</ENDPOINT>
		<VAR>?y</VAR>
		<PAGESIZE>1000</PAGESIZE>
		<RESTRICTION> </RESTRICTION>
		<PROPERTY>rdfs:label</PROPERTY>
		<TYPE>N3</TYPE>
	</TARGET>	

	<METRIC>levenshtein(x.rdfs:label, y.rdfs:label)</METRIC>

	<ACCEPTANCE>
		<THRESHOLD>0.9</THRESHOLD>
		<FILE>accepted.txt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</ACCEPTANCE>
	<REVIEW>
		<THRESHOLD>0.95</THRESHOLD>
		<FILE>reviewme.txt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</REVIEW>
	
		<EXECUTION>
		<REWRITER>default</REWRITER>
		<PLANNER>default</PLANNER>
		<ENGINE>default</ENGINE>
	</EXECUTION>
	<OUTPUT>N3</OUTPUT>
</LIMES>
